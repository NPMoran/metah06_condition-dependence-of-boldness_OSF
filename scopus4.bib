@ARTICLE{Hopewell20081143,
author={Hopewell, L.J. and Leaver, L.A. and Lea, S.E.G.},
title={Effects of competition and food availability on travel time in scatter-hoarding gray squirrels (Sciurus carolinensis)},
journal={Behavioral Ecology},
year={2008},
volume={19},
