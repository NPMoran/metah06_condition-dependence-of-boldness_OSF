@inproceedings{ ISI:000391521500046,
Author = {Gorghiu, G. and Dinu, C. and Cristea, G. C.},
Editor = {{Sandhu, A and Frunza, A and Gorghiu, G and Ciongaru, E}},
Title = {{Mass-Media - A Proper Environment for Disseminating Health Education}},
