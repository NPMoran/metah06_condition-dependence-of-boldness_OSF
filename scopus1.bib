@ARTICLE{Georgii201720,
author={Georgii, C. and Goldhofer, P. and Meule, A. and Richard, A. and Blechert, J.},
title={Food craving, food choice and consumption: The role of impulsivity and sham-controlled tDCS stimulation of the right dlPFC},
journal={Physiology and Behavior},
