@ARTICLE{Kanska201726200,
author={Kanska, J. and Aspuria, P.-J.P. and Taylor-Harding, B. and Spurka, L. and Funari, V. and Orsulic, S. and Karlan, B.Y. and Wiedemeyer, W.R.},
title={Glucose deprivation elicits phenotypic plasticity via ZEB1- mediated expression of NNMT},
