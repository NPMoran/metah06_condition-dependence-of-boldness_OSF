@ARTICLE{Parsi2002489,
author={Parsi, M.R.},
title={A world made for the measure of children and the young. The children and the young around us and the child and the adolescent within each of us [Un mondo a misura dei ragazzi e dei giovani: «Dei ragazzi e dei giovani intorno a noi e del bambino e dell'adolescente interiore dentro di noi»]},
