@ARTICLE{GonzálezMacías20031,
author={González Macías, L. and Unikel, C. and Cruz, C. and Caballero, A.},
title={Eating disorders and personality [Personalidad y trastornos de la conducta alimentaria]},
journal={Salud Mental},
