url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85009676061&doi=10.1016%2fB978-0-08-100168-4.00004-5&partnerID=40&md5=768462b13efe1913aa07f760cc3a2e05},
abstract={Physiology, lifestyle choices, and maternal nutrition before and during pregnancy have long been known to mediate fetal growth, development, and future health (. fetal programming of adult disease). Postnatal environmental cues, including infant nutrition, have also been demonstrated to induce powerful, lasting programming effects on growth trajectories, body composition, tissue differentiation, and functional development, as well as long-term health, performance, and disease risks until old age. Hence, improvements of nutrition and metabolism during the first 1000days of life, including the 270days of pregnancy and two times 365days of first two postnatal years, offer particular opportunities for preventive metabolic programming through dietary intervention because of a high degree of developmental plasticity during this early developmental period. However, preventive opportunities are not strictly limited to this 1000day window, since there is evidence for lasting programming effects of nutrition, before pregnancy as well as after the age of 2years. © 2017 Elsevier Ltd All rights reserved..},
correspondence_address1={Koletzko, B.; Ludwig-Maximilians-Universität Munich, Dr. von Hauner Children's Hospital, University of Munich Medical Center-Klinikum der Universität MünchenGermany},
isbn={9780081001868; 9780081001684},
abbrev_source_title={Early Nutr. and Long-Term Health: Mech., Conseq., and Oppor.},
document_type={Book Chapter},
source={Scopus},
}

@BOOK{Street2016495,
author={Street, S.E. and Laland, K.N.},
title={Social Learning, Intelligence, and Brain Evolution},
journal={The Wiley-Blackwell Handbook of Evolutionary Neuroscience},
year={2016},
pages={495-513},
doi={10.1002/9781118316757.ch18},
note={cited By 2},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85024396121&doi=10.1002%2f9781118316757.ch18&partnerID=40&md5=d6052906b44087d98ed4e78ab33f1f3f},
abstract={Summary: Social learning-learning influenced by observation of, or interaction with, other animals -allows individuals to acquire information, concerning, for instance, the location and quality of food, mates, predators, rivals, and pathways, as well as foraging techniques, vocalizations and a variety of social behavior. Intelligence can be broadly defined as a cross-domain measure of cognitive ability in learning, problem-solving and abstract reasoning, characterized by behavioral flexibility. Trends in vertebrate brain size evolution commonly follow simple allometric scaling rules, with individual brain regions evolving in concert with each other. Theoretical analyses demonstrate that cumulative culture and high-fidelity copying are mutually reinforcing. Currently, the nature of the relationship between social learning, brain size, and cognition is poorly understood. Theoretical models have shown that social learning is not always beneficial and can be costly, due to the risks of copying errors, receiving bad information, and information becoming outdated in a changing environment. © 2017 John Wiley & Sons, Ltd. All rights reserved.},
correspondence_address1={Street, S.E.; University of HullUnited Kingdom},
isbn={9781118316757; 9781119994695},
abbrev_source_title={The Wiley-Blackwell Handb. of Evol. Neurosci.},
document_type={Book Chapter},
source={Scopus},
}

@ARTICLE{Kadish201639,
author={Kadish, I. and Kumar, A. and Beitnere, U. and Jennings, E. and McGilberry, W. and van Groen, T.},
title={Dietary composition affects the development of cognitive deficits in WT and Tg AD model mice},
journal={Experimental Gerontology},
