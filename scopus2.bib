@ARTICLE{Hertel20161019,
author={Hertel, A.G. and Zedrosser, A. and Mysterud, A. and Støen, O.-G. and Steyaert, S.M.J.G. and Swenson, J.E.},
title={Temporal effects of hunting on foraging behavior of an apex predator: Do bears forego foraging when risk is high?},
journal={Oecologia},
year={2016},
volume={182},
